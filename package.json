{
    "name": "beano",
    "version": "0.2.2",
    "description": "A Pino inspired logger for the browser.",
    "main": "./dist/index.cjs.js",
    "module": "./dist/index.es.js",
    "browser": "./dist/index.js",
    "types": "./dist/src/index.d.ts",
    "files": [
        "dist"
    ],
    "scripts": {
        "app:start": "npm run clean && parcel app/index.html  --no-cache --no-autoinstall",
        "app:build": "npm run clean && parcel serve app/index.html --no-autoinstall",
        "clean": "rimraf dist tmp .parcel-cache",
        "publish:dist-as-docs": "rimraf docs && mv ./dist docs",
        "build": "npm run clean && rollup -c",
        "build:watch": "npm run clean && rollup -cw",
        "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
        "prettier:fix": "prettier \"{src,app}/**/*\" --write",
        "prettier:check": "prettier \"{src,app}/**/*\" --check",
        "type-check": "tsc --noEmit",
        "test": "jest",
        "all-ci": "npm run test && npm run lint && npm run prettier:check && npm run type-check && npm run build"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/erhathaway/beano.git"
    },
    "author": "Ethan Hathaway",
    "license": "MIT",
    "keywords": [
        "logger",
        "browser",
        "pino",
        "childLogger",
        "child logger",
        "module",
        "scoped",
        "group",
        "console.log",
        "console"
    ],
    "bugs": {
        "url": "https://github.com/erhathaway/beano/issues"
    },
    "homepage": "https://github.com/erhathaway/beano#readme",
    "devDependencies": {
        "@rollup/plugin-commonjs": "^11.0.2",
        "@rollup/plugin-node-resolve": "^7.1.1",
        "@types/animejs": "^3.1.0",
        "@types/jest": "^25.2.1",
        "@types/react": "^16.9.34",
        "@types/react-dom": "^16.9.5",
        "@types/react-syntax-highlighter": "^11.0.4",
        "@types/react-test-renderer": "^16.9.2",
        "@types/styled-components": "^5.0.1",
        "@typescript-eslint/eslint-plugin": "^2.26.0",
        "@typescript-eslint/parser": "^2.27.0",
        "animejs": "^3.1.0",
        "eslint": "^6.8.0",
        "eslint-config-prettier": "^6.10.1",
        "eslint-plugin-jest": "^23.8.2",
        "eslint-plugin-react": "^7.19.0",
        "husky": "^4.2.3",
        "jest": "^25.3.0",
        "mobx": "^5.15.4",
        "mobx-react": "^6.2.2",
        "parcel": "^2.0.0-alpha.3.2",
        "prettier": "^1.18.2",
        "react": "^16.13.1",
        "react-id-generator": "^3.0.0",
        "react-syntax-highlighter": "^12.2.1",
        "react-test-renderer": "^16.13.0",
        "rimraf": "^3.0.2",
        "rollup": "^2.0.6",
        "rollup-plugin-eslint": "^7.0.0",
        "rollup-plugin-external-globals": "^0.5.0",
        "rollup-plugin-typescript2": "^0.26.0",
        "rollup-watch": "^4.3.1",
        "router-primitives": "^1.0.0-beta.6",
        "styled-components": "^4.4.1",
        "ts-jest": "^25.3.0",
        "typescript": "^3.9.0",
        "react-dom": "^16.13.1"
    },
    "dependencies": {}
}
